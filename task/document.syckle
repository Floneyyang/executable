---
rdoc:
  service : rdoc
  format  : "newfish"
  output  : "site/docs/api"
  include : nil
  exclude : ["Rakefile"]
  extra   : nil

ridoc:
  output  : ".ri"
  active  : true

dnote:
  labels  : ~
  output  : ~

qedoc:
  service : custom
  cycle   : site
  document: |
    system "qedoc -o site/docs/qed -t Executable qed/"

